{
  "expo": {
    "name": "LeadTrackr",
    "slug": "leadtrackr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/leadtrackr.png",
    "scheme": "leadtrackr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/leadtrackr1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.siddhesh14.leadtrackr"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/leadtrackr.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/leadtrackr.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "814db06a-b9aa-4c67-bbf8-943a364d4073"
      }
    }
  }
}
